{
  "name": "gatsby-starter-casper",
  "description": "GatsbyJS starter that themed with the Casper theme from Ghost.",
  "version": "1.0.7",
  "author": "Hays Clark <hays.clark@gmail.com>",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.20.22",
    "gatsby-pagination": "^1.2.0",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-lodash": "^3.2.1",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-nprogress": "^2.2.1",
    "gatsby-plugin-offline": "^2.2.10",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-sitemap": "^2.3.2",
    "gatsby-plugin-twitter": "^2.2.2",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^2.0.6",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-remark-responsive-iframe": "^2.3.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-json": "^2.3.1",
    "gatsby-transformer-remark": "^5.25.1",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-disqus-comments": "^1.4.0",
    "react-dom": "^16.13.1",
    "react-gravatar": "^2.6.3",
    "react-helmet": "^5.2.1",
    "react-scroll": "^1.7.16",
    "react-share": "^1.15.1",
    "react-social-icons": "^2.9.0",
    "react-twitter-widgets": "^1.4.0"
  },
  "devDependencies": {
    "cli-glob": "^0.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "gh-pages": "^2.2.0",
    "prettier": "^1.19.1",
    "remark-cli": "^5.0.0",
    "remark-preset-lint-recommended": "^3.0.4",
    "standard-version": "^4.4.0",
    "stylefmt": "^6.0.2",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.3.0",
    "write-good": "^0.13.1"
  },
  "keywords": [
    "gatsby",
    "blog",
    "casper",
    "starter"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf .cache && rm -rf public",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:js": "eslint --ext .js,.jsx --ignore-pattern public --ignore-pattern static .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "release": "standard-version -a",
    "deploy": "gatsby build && gh-pages -d docs"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
